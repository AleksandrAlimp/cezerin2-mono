{
    "name": "cezerin2-mono",
    "version": "0.33.0",
    "description": "Cezerin is React and Node.js based eCommerce platform. https://cezerin.org",
    "keywords": [
        "cezerin",
        "cezerin2",
        "ecommerce",
        "shopping cart",
        "shop",
        "cms"
    ],
    "homepage": "https://cezerin.org",
    "author": "Himadu Sirimanna <himadusirimanna.1@gmail.com>",
    "bugs": "https://github.com/cezerin/cezerin2/issues",
    "license": "Unlicense",
    "main": "src/api/server/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/cezerin2/cezerin"
    },
    "scripts": {
        "setup": "node -r esm src/api/server/setup.js",
        "compile": "babel src/store -d dist/store --presets react --plugins transform-class-properties --quiet",
        "compile:watch": "concurrently -n storeWatcher,themeWatcher \"babel src/store -d dist/store --presets react --plugins transform-class-properties --watch\" \"babel theme/src -d theme/dist --presets react --plugins transform-class-properties --watch\"",
        "webpack:admin": "webpack --config webpack.config.admin.js --mode production",
        "webpack:store": "webpack --config webpack.config.store.js --mode production",
        "webpack:admin:watch": "webpack --config webpack.config.admin.js --mode development --watch",
        "webpack:store:watch": "webpack --config webpack.config.store.js --mode development --watch",
        "theme:install": "./scripts/theme-install.sh",
        "theme:export": "./scripts/theme-export.sh",
        "theme:build": "babel theme/src -d theme/dist --presets react --plugins transform-class-properties --quiet && npm run webpack:store",
        "build": "npm run compile && npm run theme:build && npm run webpack:admin",
        "build:watch": "concurrently -n compile,webpackStore,webpackAdmin \"npm run compile:watch\" \"npm run webpack:store:watch\" \"npm run webpack:admin:watch\"",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prettier": "prettier --write \"src/**/*.js\" \"theme/**/*.js\"",
        "precommit": "lint-staged",
        "start-api": "node -r esm src/api/server/index.js",
        "start-store": "node -r esm dist/store/server/index.js",
        "start": "concurrently npm:start-*"
    },
    "lint-staged": {
        "*.ts": [
            "prettier --write",
            "git add"
        ]
    },
    "dependencies": {
        "bcrypt": "^4.0.1",
        "body-parser": "^1.19.0",
        "cezerin2-client": "^0.36.0",
        "chart.js": "^2.9.3",
        "clean-webpack-plugin": "^3.0.0",
        "cookie-parser": "^1.4.5",
        "css-loader": "^3.5.2",
        "cssnano": "^4.1.10",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "express-jwt": "^5.3.3",
        "formidable": "^1.2.2",
        "fs": "0.0.1-security",
        "fs-extra": "^9.0.0",
        "google-spreadsheet": "^3.0.11",
        "handlebars": "^4.7.6",
        "helmet": "^3.22.0",
        "html-webpack-plugin": "^4.2.0",
        "husky": "^4.2.5",
        "jsonwebtoken": "^8.5.1",
        "lint-staged": "^10.1.3",
        "lodash": "^4.17.15",
        "lru-cache": "^5.1.1",
        "lscache": "^1.3.0",
        "material-ui": "^0.20.2",
        "mini-css-extract-plugin": "^0.9.0",
        "moment": "^2.24.0",
        "mongodb": "^3.5.5",
        "node-fetch": "^2.6.0",
        "node-sass": "^4.13.1",
        "nodemailer": "^6.4.6",
        "nodemailer-smtp-transport": "^2.7.4",
        "postcss-loader": "^3.0.0",
        "prettier": "^2.0.4",
        "prop-types": "^15.7.2",
        "query-string": "^6.12.1",
        "react": "^16.13.1",
        "react-chartjs-2": "^2.9.0",
        "react-dom": "^16.13.1",
        "react-dropzone": "^10.2.2",
        "react-helmet": "^6.0.0",
        "react-redux": "^7.2.0",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scroll": "^1.7.16",
        "react-sortable-hoc": "^1.11.0",
        "react-stripe-elements": "^6.1.1",
        "react-tagsinput": "^3.19.0",
        "redux": "^4.0.5",
        "redux-form": "^8.3.5",
        "redux-form-material-ui": "^4.3.4",
        "redux-thunk": "^2.3.0",
        "request-promise": "^4.2.5",
        "response-time": "^2.3.2",
        "sass-loader": "^8.0.2",
        "sitemap": "^6.1.1",
        "slug": "^2.1.1",
        "stripe": "^8.39.2",
        "theme": "file:theme",
        "ua-parser-js": "^0.7.21",
        "webpack": "^4.42.1",
        "webpack-cli": "^3.3.11",
        "winston": "^3.2.1",
        "workbox-webpack-plugin": "^5.1.2",
        "ws": "^7.2.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-polyfill": "^6.26.0",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-react": "^7.9.4",
        "concurrently": "^5.1.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.1.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-react": "^7.19.0"
    },
    "engines": {
        "node": ">=8.0.0",
        "npm": ">=5.0.0"
    }
}
